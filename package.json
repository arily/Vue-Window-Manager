{
  "name": "vue-window-manager",
  "version": "0.2.0",
  "private": false,
  "main": "dist/your-package-name.common.js",
  "module": "dist/your-package-name.esm.js",
  "description": "This module allows the management of windows within a Vue.js application. The windows can be moved within a parent window using drag and drop.",
  "license": "MIT",
  "scripts": {
    "dev": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "generate": "vue-cli-service build --target lib --name your-package-name --dest dist src/index.js"
  },
  "dependencies": {
    "core-js": "^3.8.3",
    "vue": "^3.2.13",
    "vue-resizable": "^2.1.7"
  },
  "keywords": [
    "Window",
    "UI",
    "TypeScript",
    "Vuejs",
    "Vue3"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/umaxiaotian/Vue-Window-Manager.git"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.16",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^8.0.3"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ]
}
