{
  "name": "vue-window-manager",
  "private": false,
  "version": "0.1.5",
  "type": "module",
  "description": "This module allows the management of windows within a Vue.js application. The windows can be moved within a parent window using drag and drop.",
  "files": [
    "dist"
  ],
  "types": "./dist/index.d.ts",
  "module": "./dist/my-vue-libs-es.js",
  "main": "./dist/my-vue-libs-umd.js",
  "license": "MIT",
  "exports": {
    ".": {
      "import": "./dist/my-vue-libs-es.js",
      "require": "./dist/my-vue-libs-umd.js"
    }
  },
  "keywords": [
    "Window",
    "UI",
    "TypeScript",
    "Vuejs",
    "Vue3"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/umaxiaotian/Vue-Window-Manager.git"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "generate": "npx vite build && npx tsc --emitDeclarationOnly"
  },
  "dependencies": {
    "typescript": "^4.9.5",
    "vue": "^3.2.47",
    "vue-resizable": "^2.1.7"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.0.0",
    "vite": "^4.1.4"
  }
}
